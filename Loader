local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Krypton", HidePremium = false, IntroText = "Krypton", SaveConfig = true, ConfigFolder = "1"})

local Tab = Window:MakeTab({
	Name = "ESP Section",
